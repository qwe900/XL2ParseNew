{
  "name": "xl2-web-server",
  "version": "1.0.0",
  "description": "NTI XL2 Web Server - USB Serial Interface",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "install-service": "sudo node install-service.js",
    "install-windows": "powershell -ExecutionPolicy Bypass -File install-windows.ps1",
    "install-windows-service": "powershell -ExecutionPolicy Bypass -File install-windows.ps1 -InstallAsService",
    "test-ports": "node -e \"import('./src/devices/XL2Connection.js').then(({XL2Connection}) => { const xl2 = new XL2Connection(); xl2.scanAllPortsForXL2().then(console.log); });\"",
    "test-gps": "node -e \"import('./src/devices/gps-logger.js').then(({default: GPSLogger}) => { const gps = new GPSLogger(); gps.scanForGPS().then(console.log); });\""
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-writer": "^1.6.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "gps": "^0.6.1",
    "helmet": "^7.0.0",
    "serialport": "^12.0.0",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "xl2",
    "serial",
    "web-interface",
    "audio-analyzer"
  ],
  "author": "Your Name",
  "license": "MIT"
}
